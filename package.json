{
  "name": "future-of-qe-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    
    "test:e2e": "playwright test --project=e2e --headed",
    "test:e2e:ui": "playwright test --project=e2e --ui",
    "test:e2e:debug": "playwright test --project=e2e --debug",
    
    "test:functional": "playwright test --project=functional --headed",
    "test:functional:ui": "playwright test --project=functional --ui",
    
    "test:performance": "playwright test --project=performance --headed",
    "test:performance:ui": "playwright test --project=performance --ui",
    
    "test:visual": "playwright test --project=visual --headed",
    "test:visual:ui": "playwright test --project=visual --ui",
    "test:visual:update": "playwright test --project=visual --update-snapshots",
    "test:visual:debug": "playwright test --project=visual --debug",
    
    "test:all": "playwright test --project=functional --project=e2e --project=performance",
    "test:all:headless": "playwright test --project=functional --project=e2e --project=performance",
    "test:complete": "playwright test --project=functional --project=e2e --project=performance --project=visual",
    "test:complete:headless": "playwright test --project=functional --project=e2e --project=performance --project=visual",
    
    "test:report": "playwright show-report",
    
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@mui/styled-engine": "^7.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "framer-motion": "^12.23.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "lighthouse": "^12.8.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.17",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}